<?xml version="1.0" encoding="UTF-8"?> 
<!DOCTYPE hibernate-mapping 
PUBLIC  "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="android.hcl.bean.Book" table="Book">
		<id name="bookId" column="bookId" type="java.lang.Integer">
			<generator class="identity"></generator>
		</id>
		<property name="username" column="username" type="java.lang.String" />
		<property name="phone" column="phone" type="java.lang.String" />
		<property name="address" column="address" type="java.lang.String" />
		<property name="busunessname" column="busunessname" type="java.lang.String" />
		<property name="dishName" column="dishName" type="java.lang.String" />
		<property name="number" column="number" type="java.lang.Integer" />
		<property name="price" column="price" type="java.lang.Float" />
		<property name="totalconsumption" column="totalconsumption"
			type="java.lang.Float" />
		<property name="bookTime" column="bookTime" type="java.sql.Timestamp" />
		<property name="bookFinish" column="bookFinish" type="java.lang.String" />
	</class>
</hibernate-mapping>